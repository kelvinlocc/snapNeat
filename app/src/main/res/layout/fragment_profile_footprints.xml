<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    tools:context="com.accordhk.SnapNEat.fragments.ProfileFootprintsFragment">

    <com.accordhk.SnapNEat.utils.CustomFontTextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/message"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:textSize="@dimen/font_38" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:paddingBottom="@dimen/margin_bottom_50"
        android:id="@+id/ll_footprints" />

</FrameLayout>
